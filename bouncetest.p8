pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
	x = 5
	y = 0
	dx = 0
	dy = 0
	gravity = 2
	friction = 1.2
end

function _update()
	
	if btnp(4) then
		dy = -10
	end
	
	dy = dy*0.8
	
	x += dx/friction
	y += dy + gravity
	testing = y
	
	x = mid(0,x,127)
	y = mid(0,y,100)
end

function _draw()
	cls()
	pset(x,y,13)
	print(dy)
 print(y)
 print(testing)
end
