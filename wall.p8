pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
-- wall bounce tests
-- a thoughtless labs experiment

function _init()
	bx = 50
	by = 0
	bdx = 0
	bdy = 0
	bdx_max = 15
	flipx = false
	flipy = false
	power=0
end

function _update60()
	if btn(0) or btn(1) then
	charging = true
	powerused = false
		if abs(power) < bdx_max then
			if btn(0) then
				power -= 0.5	
			elseif btn(1) then
				power +=0.5
			end
		end
	else
		charging = false
	end

	if btnp(2) then
		if by > 105 then
			bdy = -10
		end
	end
	
	if by == 110 then
		bdx /=1.05
	end
	
	if abs(bdx) < 0.05 then 
		bdx=0
	end
	
	if not charging then
		bdx = bdx + power
		powerused = true
	end
	
	if powerused then
		power = 0
	end
	
	bdy += 0.3
	bx += bdx
	by += bdy + 0.8
	
	check_collision(bx,by)

	bx = mid(5,bx,120)
	by = mid(5,by,110)
end

function _draw()
	cls()
	spr(1,bx,by,1,1,flipx,flipy)
	print([[
	⬆️ to jump
	
	hold ⬅️ and ➡️ to set
	power for smacking it
	]]) 
end

function check_collision(x,y)
	if x > 120 or x < 5 then
		bdx = -bdx
	end
	if y < 5 or y >= 110 then
			bdy = -bdy
	end
end
